  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "monorepo": {
    "productionBranch": "master",
    "developBranch": "master",
    "npmAccess": "public",
    "ghpages": [
      {
        "dir": "./packages/storybook/.out",
        "url": "https://github.com/tsedio/tsed-formio.git",
        "branch": "gh-pages",
        "cname": "formio.tsed.io"
