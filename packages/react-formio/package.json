{
  "name": "@tsed/react-formio",
  "version": "1.10.13",
  "description": "Provide a react formio wrapper. Written in TypeScript.",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "test": "cross-env CI=1 yarn craco test --coverage",
    "build": "microbundle --no-compress --format modern,cjs --jsx React.createElement",
    "watch": "microbundle watch --no-compress --format modern,cjs --jsx React.createElement",
    "lint:fix": "yarn lint --fix",
    "lint": "yarn eslint --config .eslintrc --cache --quiet \"./src/**/*.{ts,tsx}\"",
    "prettier": "prettier '{src,test}/**/*.{ts,tsx}' --write"
  },
  "dependencies": {
    "@tsed/redux-utils": "1.10.13",
    "eventemitter2": "^6.4.3",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "choices.js": ">=9.0.1",
    "formiojs": ">=4.0.0",
    "lodash": ">=4.17.20",
    "prop-types": ">=15.7.2",
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0",
    "react-table": ">=7.6.3",
    "tooltip.js": ">=1.3.3"
  },
  "devDependencies": {
    "@tsed/tailwind": "1.10.13",
    "@tsed/tailwind-formio": "1.10.13",
    "eslint-plugin-jsx-a11y": "^6.5.1"
  }
}
