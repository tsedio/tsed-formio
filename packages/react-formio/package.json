{
  "name": "@tsed/react-formio",
  "version": "3.0.0-alpha.7",
  "description": "Provide a react formio wrapper. Written in TypeScript.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.modern.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "tsed-source": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "tsed-source": "./src/*",
      "default": "./dist/*.js"
    }
  },
  "source": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "test": "vitest run --coverage",
    "build": "vite build",
    "watch": "vite build --watch"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "eventemitter2": "^6.4.3",
    "use-debounce": "^10.0.4"
  },
  "peerDependencies": {
    "@formio/choices.js": ">=9.0.1",
    "formiojs": ">=4.0.0",
    "lodash": ">=4.17.20",
    "moment": ">=2.30.1",
    "react": ">=16.14.0",
    "react-dnd": ">=16.0.1",
    "react-dnd-html5-backend": ">=16.0.1",
    "react-dom": ">=16.14.0",
    "react-select": ">=5.9.0",
    "react-table": ">=7.6.3",
    "tooltip.js": ">=1.3.3"
  },
  "devDependencies": {
    "@tsed/tailwind-formio": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "microbundle": "0.13.0",
    "vite": "5.4.11",
    "vitest": "2.1.8"
  }
}
